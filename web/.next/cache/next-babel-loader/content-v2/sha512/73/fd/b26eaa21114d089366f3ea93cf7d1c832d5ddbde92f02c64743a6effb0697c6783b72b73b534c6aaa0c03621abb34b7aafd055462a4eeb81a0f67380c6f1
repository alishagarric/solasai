{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\n// Core\nimport React from \"react\"; // Styles\n\nimport { ButtonClassName, ButtonStyle } from \"./styles.scss\"; //Components\n\nimport AnchorLink from 'react-anchor-link-smooth-scroll';\nimport { Download } from \"../../_svg/Icons/Download\"; // Begin Component\n// __________________________________________________________________________________________\n\n/**\n *\n * @name Button\n * @author Alisha Garric\n * @requires /studio/sections/Button\n *\n */\nexport const Button = ({\n  link,\n  label,\n  download,\n  target\n}) => {\n  if (download) {\n    return /*#__PURE__*/_jsx(ButtonStyle, {\n      className: `${ButtonClassName}`,\n      children: /*#__PURE__*/_jsxs(\"a\", {\n        className: \"txt-caption\",\n        href: download,\n        download: true,\n        children: [/*#__PURE__*/_jsx(Download, {}), label]\n      })\n    });\n  } else if (link && !target) {\n    return /*#__PURE__*/_jsx(ButtonStyle, {\n      className: `${ButtonClassName}`,\n      children: /*#__PURE__*/_jsx(AnchorLink, {\n        href: link,\n        className: \"txt-caption\",\n        children: label\n      })\n    });\n  } else if (link && target) {\n    return /*#__PURE__*/_jsx(ButtonStyle, {\n      className: `${ButtonClassName}`,\n      children: /*#__PURE__*/_jsx(\"a\", {\n        href: link,\n        target: target ? target : \"_self\",\n        className: \"txt-caption\",\n        children: label\n      })\n    });\n  } else {\n    return /*#__PURE__*/_jsx(ButtonStyle, {\n      className: `${ButtonClassName}`,\n      children: /*#__PURE__*/_jsx(\"div\", {\n        className: \"txt-caption\",\n        children: label\n      })\n    });\n  }\n}; // End Component\n// __________________________________________________________________________________________","map":null,"metadata":{},"sourceType":"module"}